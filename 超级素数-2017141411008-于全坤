#include <stdio.h>

int IsPrimeNumber(int n)

{

	int i,j;

	for(i=2;i<=n/2;i++)

	{

		if(n%i==0)

		{

			j=0;

			break;

		}

		else

		{

			j=1;

		}

	}

	return j;

 } 

 int Split1(int n)

 {

 	int i,j,k,m;

 	i=n/100;

 	j=(n-i*100)/10;

 	k=(n-i*100-j*10);

 	m=i*i+j*j+k*k;

	 return m; 

 }

 int Split2(int n)

 {

 	int i,j,k,m;

 	i=n/100;

 	j=(n-i*100)/10;

 	k=(n-i*100-j*10);

 	m=i+j+k;

	 return m; 

 }

 int Split3(int n)

 {

 	int i,j,k,m;

 	i=n/100;

 	j=(n-i*100)/10;

 	k=(n-i*100-j*10);

 	m=i*j*k;

	 return m; 

 }

 int main()

 {

 	int i,j=0,result=0;

 	int n,p,q,r;

 	for(i=100;i<=999;i++)

 	{

 		p=Split1(i);

 		q=Split2(i);

 		r=Split3(i);

 		if(IsPrimeNumber(i)&&IsPrimeNumber(p)&&IsPrimeNumber(q)&&IsPrimeNumber(r))

 		{

 			j++;

 			printf("%d\t",i);

 			result+=i;

			  

		 }

		 else

		 ;

	 }

	 printf("\n%d %d\n",j,result);

	 return 0;

 }
